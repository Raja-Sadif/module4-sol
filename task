// define array of names
var names = ["John", "Jane", "Jim", "Jack", "Jill", "Julie", "Mike", "Mary"];

// loop over the names and print out either hello or goodbye
for (var i = 0; i < names.length; i++) {
  var name = names[i];
  var firstLetter = name.charAt(0).toLowerCase();
  
  if (firstLetter === 'j') {
    console.log("Goodbye " + name);
  } else {
    console.log("Hello " + name);
  }
}
